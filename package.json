{
  "name": "simple-universal-react-redux",
  "version": "0.10.1",
  "description": "The simplest Async Universal React & Redux boilerplate I could possibly make.",
  "main": "src/server/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev-server": "cross-env BABEL_ENV=server NODE_ENV=development nodemon ./src/server/index.js --exec babel-node --ignore dist/ -e js,scss,css",
    "dev-client": "cross-env NODE_ENV=development webpack --config ./webpack.config.dev.js",
    "dev": "concurrently \"cross-env NODE_ENV=development npm run dev-server\" \"cross-env NODE_ENV=development npm run dev-client\"",
    "prod-server": "cross-env BABEL_ENV=server NODE_ENV=production babel src --out-dir ./dist",
    "prod-client": "cross-env NODE_ENV=production webpack --config ./webpack.config.prod.js",
    "build": "npm run clean && npm run prod-server && npm run prod-client",
    "start": "cross-env NODE_ENV=production node ./dist/server/index.js"
  },
  "author": "William Woodhead",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/william-woodhead/simple-universal-react-redux"
  },
  "homepage": "https://github.com/william-woodhead/simple-universal-react-redux",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "redux",
    "boilerplate",
    "babel",
    "express",
    "react-router"
  ],
  "dependencies": {
    "express": "^4.16.4",
    "loader-utils": "^1.2.3",
    "lodash": "^4.17.11",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "redux-logger": "^3.0.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  }
}
